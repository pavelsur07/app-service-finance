{{ form_start(form) }}
    <div class="row">
        <div class="col-md-4">
            {{ form_row(form.code) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.name) }}
        </div>
        <div class="col-md-2">
            {{ form_row(form.isActive) }}
        </div>
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">{{ button_label|default('Сохранить') }}</button>
        <a href="{{ path('deal_charge_type_index') }}" class="btn btn-outline-secondary ms-2">К списку</a>
    </div>
{{ form_end(form) }}
